<% if @cocktail.favorited_by?(current_user) %>
  <%= link_to "Unfavorite ", cocktail_favorite_path(@cocktail, @cocktail.favorite_for(current_user)), method: :delete, data: {disable_with: "..."} %>|
<% else %>
  <%= link_to "Favorite ", cocktail_favorites_path(@cocktail), method: :post %>|
<% end %>
